{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    },
    "cloudflare-docs": {
      "command": "npx",
      "args": ["mcp-remote", "https://docs.mcp.cloudflare.com/mcp"]
    },
    "cloudflare-dns": {
      "type": "http",
      "url": "https://dns-analytics.mcp.cloudflare.com/mcp"
    },
    "Railway": {
      "type": "stdio",
      "command": "npx",
      "args": ["@railway/mcp-server"],
      "env": {}
    },
    "logfire": {
      "type": "http",
      "url": "https://logfire-us.pydantic.dev/mcp"
    },
    "quo": {
      "type": "http",
      "url": "https://mcp.quo.com/sse"
    }
  }
}
